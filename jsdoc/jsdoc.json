{
	"plugins": [
		"plugins/markdown"
	],
	"source": {
		"include": [
			"cheatgui.js"
		]
	},
	"opts": {
		"template": "node_modules/clean-jsdoc-theme",
		"readme": "./README.md",
		"encoding": "utf8",
		"destination": "./jsdoc/out",
		"recurse": true,
		"theme_opts": {
			"default_theme": "dark",
			"homepageTitle": "CheatGUI",
			"title": "CheatGUI",
			"menu": [
				{
					"title": "GitHub",
					"link": "https://github.com/cat-125/cheatgui",
					"target": "_blank"
				},
				{
					"title": "Demo",
					"link": "https://cat-125.github.io/cheatgui/demo",
					"target": "_blank"
				}
			],
			"meta": [
				{
					"name": "author",
					"content": "Cat-125"
				},
				{
					"name": "description",
					"content": "Effortless library for building window-based interfaces."
				}
			]
		}
	},
	"markdown": {
		"hardwrap": false,
		"idInHeadings": true
	}
}
